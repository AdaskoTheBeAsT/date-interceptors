{
  "name": "hierarchical-convert-to-luxon",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/hierarchical-convert-to-luxon/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/hierarchical-convert-to-luxon",
        "tsConfig": "packages/hierarchical-convert-to-luxon/tsconfig.lib.json",
        "packageJson": "packages/hierarchical-convert-to-luxon/package.json",
        "main": "packages/hierarchical-convert-to-luxon/src/index.ts",
        "assets": ["packages/hierarchical-convert-to-luxon/*.md"]
      }
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs hierarchical-convert-to-luxon {args.ver} {args.tag}",
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/hierarchical-convert-to-luxon/**/*.ts",
          "packages/hierarchical-convert-to-luxon/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/hierarchical-convert-to-luxon/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
