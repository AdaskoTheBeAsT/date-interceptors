{
  "name": "angular-date-http-interceptor",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/angular-date-http-interceptor/src",
  "prefix": "adaskothebeast",
  "tags": [],
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "packages/angular-date-http-interceptor/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "packages/angular-date-http-interceptor/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "packages/angular-date-http-interceptor/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/angular-date-http-interceptor/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/angular-date-http-interceptor/**/*.ts",
          "packages/angular-date-http-interceptor/**/*.html"
        ]
      }
    }
  }
}
